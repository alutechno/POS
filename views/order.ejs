    function randomData() {
        var startId = ~~(Math.random() * 100),
            rows = [];

        for (var i = 0; i < 10; i++) {
            if (i % 2) {
                rows.push({
                    id: startId + i,
                    name: 'test apaan coba deh' + (startId + i),
                    qty: i,
                    amount: '8,881,123.00'
                });
			} else {
                rows.push({
                    id: startId + i,
                    name: 'test apaan coba deh' + (startId + i),
                    qty: i,
                    amount: startId * i
                });
			}
        }
        return rows;
    }
    function rowStyle(row, index) {
        var classes = ['active', 'success', 'info', 'warning', 'danger'];

        if (index % 2 === 0 && index / 2 < classes.length) {
            return {
                classes: classes[index / 2]
            };
        }
        return {};
    }
</script>
